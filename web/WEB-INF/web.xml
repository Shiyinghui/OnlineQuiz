<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.system.servlet.do_login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.system.servlet.do_register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>gamerRegister</servlet-name>
        <servlet-class>com.system.servlet.do_gamerRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gamerRegister</servlet-name>
        <url-pattern>/gamerRegister</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>adminRegister</servlet-name>
        <servlet-class>com.system.servlet.do_adminRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminRegister</servlet-name>
        <url-pattern>/adminRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addQuestion</servlet-name>
        <servlet-class>com.system.servlet.do_addQ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addQuestion</servlet-name>
        <url-pattern>/Admin/addQuestion</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>uploadFile</servlet-name>
        <servlet-class>com.system.servlet.do_uploadFile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadFile</servlet-name>
        <url-pattern>/upLoadFile</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>modifyQuestion</servlet-name>
        <servlet-class>com.system.servlet.do_modifyQ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>modifyQuestion</servlet-name>
        <url-pattern>/Admin/modifyQuestion</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>deleteQuestion</servlet-name>
        <servlet-class>com.system.servlet.do_deleteQ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deleteQuestion</servlet-name>
        <url-pattern>/Admin/deleteQuestion</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addAnnounce</servlet-name>
        <servlet-class>com.system.servlet.do_addAnnounce</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addAnnounce</servlet-name>
        <url-pattern>/Admin/addAnnounce</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>deleteGamer</servlet-name>
        <servlet-class>com.system.servlet.do_deleteGamer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deleteGamer</servlet-name>
        <url-pattern>/Admin/deleteGamer</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>handleQuiz</servlet-name>
        <servlet-class>com.system.servlet.do_handleQuiz</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>handleQuiz</servlet-name>
        <url-pattern>/Gamer/handleQuiz</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addLibrary</servlet-name>
        <servlet-class>com.system.servlet.do_QLibrary</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addLibrary</servlet-name>
        <url-pattern>/Admin/addLibrary</url-pattern>
    </servlet-mapping>


</web-app>

